{
  "name": "mojing-script",
  "displayName": "MoJing-Script-Hinter",
  "description": "An extension for Project MoJing",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MapleLeafGitHub/MoJingScriptHinter-VSCode"
  },
  "publisher": "lirewriter",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "icon.png",
  "contributes": {
    "languages": [{
      "id": "mjscript",
      "aliases": ["mjscript", "mjscript"],
      "extensions": [".mjs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mjscript",
      "scopeName": "main.mjs",
      "path": "./syntaxes/mjscript.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "mjscript", 
        "path": "snippets/mjs.code-snippets" 
      }
    ]
  }
  
}
